<%= render "layouts/header" %>
<p id="notice"><%= notice %></p>

<div class="container">
    <% provide(:title, @user.first_name) %>

    <div class="top-thumbnail profile-banner text-center">
			<%= image_tag "your-profile.png", class:"img-responsive" %>
			<div class="top-caption">
				<h1>  <%= @user.first_name %>'s events</h1>

			</div>
		</div>
    <div class="row">
        <section class="user_info text-center">
            <h1 id="Person-name">

            </h1>
        </section>

    </div>
        <% @active_tab = 0 %>
        <%= render partial: 'events/user_events' %>

</div>
